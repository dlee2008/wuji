{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-focus",
    "core:window:allow-show",
    "core:window:allow-close",
    "shell:allow-open",
    "shell:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://*"
        }
      ]
    },
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-entries",
    "store:allow-has",
    "store:allow-length",
    "store:allow-reset",
    "fetch-plugin:default",
    "os:default",
    "core:window:allow-set-min-size",
    "core:window:allow-destroy",
    "core:window:allow-hide",
    "core:window:allow-get-all-windows"
  ]
}
